{{define "contents"}}
<div class="contents">
    <h2>Contents</h2>

    <div class="contents">
    {{range $indexData := .}}
        <h3><a href="{{$indexData.Ref}}#">{{$indexData.Title}}</a></h3>
            <div class="scenario">
            {{range $testData := .TestData}}
            {{if $testData.TestResult.Failed}}
                <div class='test-failed'><a href="{{$indexData.Ref}}#{{$testData.TestResult.TestID}}">{{$testData.TestTitle}}</a></div>
            {{else if $testData.TestResult.Skipped}}
                <div class='test-not-run'><a href="{{$indexData.Ref}}#{{$testData.TestResult.TestID}}">{{$testData.TestTitle}}</a></div>
            {{else}}
                <div class='test-passed'><a href="{{$indexData.Ref}}#{{$testData.TestResult.TestID}}">{{$testData.TestTitle}}</a></div>
            {{end}}
            {{end}}
            </div>
    {{end}}
    </div>
</div>
{{end}}